        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler">
          <stringProp name="BeanShellSampler.query">// Define the simulated response data
String response = &quot;This is Simulated API response&quot;;
 
// Set the response data for the sampler
SampleResult.setResponseData(response, &quot;UTF-8&quot;);
 
// Set the HTTP response code
SampleResult.setResponseCode(&quot;200&quot;);
 
// Set the response message
SampleResult.setResponseMessage(&quot;OK&quot;);

// Mark the sampler as successful
SampleResult.setSuccessful(true);
 
// Set data type
SampleResult.setDataType(SampleResult.TEXT);
 
// Set content type
SampleResult.setContentType(&quot;application/json&quot;);</stringProp>
        </BeanShellSampler>
